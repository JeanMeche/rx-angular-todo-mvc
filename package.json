{
  "name": "todo-mvc",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:client": "ng serve",
    "start:server": "nodemon server.js",
    "start": "concurrently \"yarn start:server\" \"yarn start:client\"",
